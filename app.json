{
  "expo": {
    "name": "AwayApp",
    "slug": "AwayApp",
    "version": "1.0.0",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["*/"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": { "apiKey": "AIzaSyBxu_7vriZr_HochNOZmK64QTsbm4UhyZA" }
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.karang6.AwayApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photoPermission": "The app needs access to your camera roll so you can put pictures of your items on the app.",
          "cameraPermission": "The app needs access to your camera so you can take pictures of your items to put on the app."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "dd78721a-595f-4c31-9d80-c5f2100c146a"
      }
    }
  }
}
